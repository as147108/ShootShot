@model ShootShot.Models.tMemberPhot
@{
    ViewBag.Title = "EditPhotoGrapherProfile";
}

<head>
    <link rel="stylesheet" href="~/Content/css/phoMgmt.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
</head>
<div class="divnavbar"></div>
<!-- #article 左側欄按鈕區塊 -->
<article id="member_artForbtn" name="member_artForbtn" class="member_artForbtn">
    <div class="leftbutton">

        <button id="member_btnMInfo" name="member_btnMInfo">
            <a href="/MemberCenter/EditProfile" id="member_btnPjtManage" name="member_btnPjtManage">會員基本資料</a>
        </button>

        <button id="member_btnPjtManage" name="member_btnPjtManage">
            <a href="./prjMgmt.html">專案管理</a>
        </button>

        <button class="pageone" id="member_btnPOpen" name="member_btnPOpen">
            <a href="/PhotoGrapher/EditPhotoGrapherProfule" id="member_alinkPOpen" name="member_alinkPOpen">攝影師</a>
        </button>

        <!-- 開通攝影師功能按鈕start -->
        <button class="pagetwo  photographer" id="member_btnPOpen" name="member_btnPOpen">
            <a href="/PhotoGrapher/EditPhotoGrapherProfule" id="member_alinkPOpen" name="member_alinkPOpen">攝影師基本資料</a>
        </button>
        <button class="photographer" id="member_btnPOpen" name="member_btnPOpen">
            <a href="./phoAlbum.html" id="member_alinkPOpen" name="member_alinkPOpen">我的相簿</a>
        </button>
        <button class="photographer" id="member_btnPOpen" name="member_btnPOpen">
            <a href="./phoPrjMgmt.html" id="member_alinkPOpen" name="member_alinkPOpen">預約管理</a>
        </button>
        <!-- 開通攝影師功能按鈕end -->

        <button id="member_btnBackIndex" name="member_btnBackIndex">
            <a href="/Home/Index">回首頁</a>
        </button>
    </div>

</article>



<!-- #article 右側欄內容：會員資料區塊 -->
<article class="phoMember_artForForm">

    <div class="phoMember_divForForm">

        <form action="~/PhotoGrapher/EditPhotoGrapherProfile" method="post">
            <!-- 姓名 -->
            <div class="phoMember_divForName">
                <label for="">公開名稱 </label>
                <input name="fName" id="publicName" type="text" value="@Model.fName" />
                <a id="useOldName" href="#">使用本名</a>
            </div>

            <!-- 自我介紹-->
            <div class="phoMember_divForIntro">
                <label for="">自我介紹 </label>
                <textarea ame="fIntro" id="" cols="75" rows="5">@Model.fIntro</textarea>
            </div>

            <!-- 工作室名稱-->
            <div class="phoMember_divForStu">
                <label for="">工作室名稱 </label>
                <input name="fStudioName" type="text" value="@Model.fStudioName" />

            </div>

            <!-- 個人網站及攝影連結 -->
            <label for="">個人網站及攝影連結 </label>

            <div class="phoMember_divForWeb">

                <!-- FB -->
                <svg viewBox="-110 1 511 512" xmlns="http://www.w3.org/2000/svg">
                    <path d="M180 512H98.008c-13.695 0-24.836-11.14-24.836-24.836V302.227H25.336C11.64 302.227.5 291.082.5 277.39v-79.246c0-13.696 11.14-24.836 24.836-24.836h47.836v-39.684c0-39.348 12.355-72.824 35.726-96.805C132.375 12.73 165.184 0 203.778 0l62.53.102c13.672.023 24.794 11.164 24.794 24.835v73.579c0 13.695-11.137 24.836-24.829 24.836l-42.101.015c-12.84 0-16.11 2.574-16.809 3.363-1.152 1.31-2.523 5.008-2.523 15.223v31.352h58.27c4.386 0 8.636 1.082 12.288 3.12 7.88 4.403 12.778 12.727 12.778 21.723l-.031 79.247c0 13.687-11.141 24.828-24.836 24.828h-58.47v184.941C204.84 500.86 193.696 512 180 512zm-76.813-30.016h71.633V288.79c0-9.144 7.442-16.582 16.582-16.582h66.727l.027-68.883h-66.758c-9.14 0-16.578-7.437-16.578-16.582v-44.789c0-11.726 1.192-25.062 10.043-35.086 10.696-12.117 27.551-13.515 39.301-13.515l36.922-.016V30.109l-57.332-.093c-62.024 0-100.566 39.703-100.566 103.609v53.117c0 9.14-7.438 16.582-16.579 16.582H30.516v68.883h56.093c9.141 0 16.579 7.438 16.579 16.582zM266.25 30.117h.004zm0 0"
                          fill="#242424" />
                </svg>
                <input  name="fFB" type="url" value="@Model.fFB" id="vector" placeholder="https://" />
            </div>
            <div class="phoMember_divForWeb">

                <!-- IG -->
                <svg width="36" height="36" viewBox="0 0 512.001 512.001"
                     xmlns="http://www.w3.org/2000/svg">
                    <path d="M373.406 0H138.594C62.172 0 0 62.172 0 138.594V373.41C0 449.828 62.172 512 138.594 512H373.41C449.828 512 512 449.828 512 373.41V138.594C512 62.172 449.828 0 373.406 0zm108.578 373.41c0 59.867-48.707 108.574-108.578 108.574H138.594c-59.871 0-108.578-48.707-108.578-108.574V138.594c0-59.871 48.707-108.578 108.578-108.578H373.41c59.867 0 108.574 48.707 108.574 108.578zm0 0"
                          fill="#242424" />
                    <path d="M256 116.004c-77.195 0-139.996 62.8-139.996 139.996S178.804 395.996 256 395.996 395.996 333.196 395.996 256 333.196 116.004 256 116.004zm0 249.976c-60.64 0-109.98-49.335-109.98-109.98 0-60.64 49.34-109.98 109.98-109.98 60.645 0 109.98 49.34 109.98 109.98 0 60.645-49.335 109.98-109.98 109.98zM399.344 66.285c-22.813 0-41.367 18.559-41.367 41.367 0 22.813 18.554 41.371 41.367 41.371s41.37-18.558 41.37-41.37-18.558-41.368-41.37-41.368zm0 52.719c-6.258 0-11.352-5.094-11.352-11.352 0-6.261 5.094-11.351 11.352-11.351 6.261 0 11.355 5.09 11.355 11.351 0 6.258-5.094 11.352-11.355 11.352zm0 0"
                          fill="#242424" />
                </svg>
                <input name="fIns" type="url" value="@Model.fIns" id="vector" placeholder="https://" />
            </div>

            <div class="phoMember_divForSub">
                <img class="submit" src="~/Content/img/arrows.svg" alt="">
                <input class="submit" type="submit" value="資料修改">
            </div>
        </form>
    </div>
</article>
<script>
    $("#useOldName").click(function () {
            $("#publicName").val('@ViewBag.oldName2');
        })
</script>