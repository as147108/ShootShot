	<title>SHOOTSHOT</title>
	<meta content="width=device-width, initial-scale=1.0" name="viewport">
	<meta content="SHOOTSHOT" name="keywords">
	<meta content="SHOOTSHOT" name="description">
	<!-- #endregion -->
	<!-- #region Icon請勿更改 -->
	<link href="~/Content/images/logo.PNG" rel="icon">
	<!-- #endregion -->
	<!-- #region 共用googleFont請勿更改 -->
