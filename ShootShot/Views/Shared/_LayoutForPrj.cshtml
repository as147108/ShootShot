					<a @Session[Dictionary.NAVLINK_HIDDEN] href="" class="nav-item nav-link">預約專案</a>
					<a @Session[Dictionary.NAVLINK_HIDDEN] href="" class="nav-item nav-link">專案管理</a>
					<a @Session[Dictionary.LOGOUT_HIDDEN] href="/LoginAndSignup/Logout" class="nav-item nav-link">登出</a>
				</div>
			</div>
		</div>
	</nav>
	@*#endregion*@

	@*#region 妳的html*@
	<div class="divnavbar"></div>
