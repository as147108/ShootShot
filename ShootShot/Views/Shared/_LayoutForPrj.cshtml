			<button id="member_btnPOpen" name="member_btnPOpen">
				<a href="./phoVerif.html" id="member_alinkPOpen" name="member_alinkPOpen">攝影師</a>
			</button>

			<button id="member_btnBackIndex" name="member_btnBackIndex">
				<a href="./index.html">回首頁</a>
			</button>
		</div>
	</article>
	<article>

		@RenderBody()
